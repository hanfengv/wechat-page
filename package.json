{
  "name": "wechat-page",
  "displayName": "wechat-page",
  "description": "微信小程序 page 关联文件",
  "publisher": "hanfengv",
  "repository": "https://github.com/hanfengv/wechat-page",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.58.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:wechat-page.createTpl",
    "onCommand:wechat-page.wePage",
    "onCommand:wechat-page.helloWorld"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "wechat-page.createTpl",
        "title": "wechat-page[createTpl]: 创建模板到此处"
      },
      {
        "command": "wechat-page.wePage",
        "title": "wechat-page[wePage]: 显示与当前文件关联的 page"
      },
      {
        "command": "wechat-page.helloWorld",
        "title": "wechat-page[HelloWorld]"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "explorerResourceIsFolder",
          "command": "wechat-page.createTpl",
          "group": "navigation",
          "label": "创建模板"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.58.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  }
}
